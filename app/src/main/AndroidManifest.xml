<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.century22">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Century22">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".view.AgentsActivity"><!-- .view.PropertyActivity -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.ProfileActivity"
            android:parentActivityName=".view.AgentsActivity"
            />
        <activity
            android:name=".view.GoogleMapsActivity"
            android:parentActivityName=".view.ProfileActivity"
            />
        <activity
            android:name=".view.PropertiesActivity"
            android:parentActivityName=".view.AgentsActivity"
            />
        <activity
            android:name=".view.AddPropertyActivity"
            android:parentActivityName=".view.PropertiesActivity"
            />
        <activity
            android:name=".view.PropertyDetailActivity"
            android:parentActivityName=".view.PropertiesActivity"
            />
    </application>
</manifest>